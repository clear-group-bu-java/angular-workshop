  "components": {
    "schemas": {
      "Person": {
        "title": "Person",
        "type": "object",
        "x-examples": {
          "example-1": {
            "id": "497f6eca-6276-4993-bfeb-53cbbbba6f08",
            "firstName": "string",
            "lastName": "string",
            "email": "user@example.com",
            "birthDate": "2019-08-24",
            "": {
              "id": "497f6eca-6276-4993-bfeb-53cbbbba6f08",
              "street": "string",
              "postcode": "string",
              "city": "string"
            }
          }
        },
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "firstName": {
            "type": "string",
            "minLength": 1,
            "x-faker": "name.firstName"
          },
          "lastName": {
            "type": "string",
            "minLength": 1,
            "x-faker": "name.lastName"
          },
          "email": {
            "type": "string",
            "format": "email",
            "x-faker": "internet.email"
          },
          "birthDate": {
            "type": "string",
            "format": "date"
          },
          "cv": {
            "type": "string",
            "default": "",
            "x-faker": "lorem.paragraphs"
          }
        },
        "required": [
          "firstName",
          "lastName",
          "email",
          "cv"
        ]
      },
      "Address": {
        "title": "Address",
        "type": "object",
        "x-examples": {},
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "street": {
            "type": "string",
            "x-faker": "address.streetName",
            "minLength": 1
          },
          "number": {
            "type": "string",
            "x-faker": "random.number"
          },
          "city": {
            "$ref": "#/components/schemas/City"
          }
        },
        "required": [
          "street",
          "number",
          "city"
        ]
      },
      "City": {
        "title": "City",
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "x-faker": "address.city"
          },
          "zipCode": {
            "type": "string",
            "x-faker": "address.zipCode"
          }
        },
        "required": [
          "name",
          "zipCode"
        ]
      }
    },
    "securitySchemes": {}
